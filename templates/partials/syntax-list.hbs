{{#each syntaxList ~}}
  {{~#if (isKind this.kind 'InterfaceDeclaration')}}
    {{~>interface this fileName=../fileName}}
  {{else if (isKind this.kind 'TypeAliasDeclaration')}}
    {{~>type-literal this fileName=../fileName}}
  {{else if (isKind this.kind 'EnumDeclaration')}}
    {{~>enum this fileName=../fileName}}
  {{~/if}}

{{/each}}
