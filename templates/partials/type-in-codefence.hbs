{{#if (isEqual kind 163)}}{{! PropertySignature ~}}
  {{~>property-signature this}}
{{~else if (isEqual kind 174)}}{{! TypeReference ~}}
  {{~typeName~}}
  {{~#if typeArguments~}}<
    {{~#each typeArguments~}}
      {{~>type-in-codefence this~}}{{#unless @last}}, {{/unless}}
    {{~/each}}>
  {{~/if}}
{{~else if (isEqual kind 178)}}{{! TypeLiteral~}}
  {{~#each members}}
    {{~>property-signature this path=../path}}
  {{~/each}}
{{~else if (isEqual kind 179)}}{{! ArrayType ~}}
  Array<{{elementType.typeName}}>
{{~else if (isEqual kind 180)}}{{! TupleType ~}}
  [{{#each elements~}}
    {{~>type-in-codefence this}}{{#unless @last}}, {{/unless}}
  {{~/each}}]
{{~else if (isEqual kind 183)}}{{! UnionType ~}}
  {{~#each types~}}
    {{>type-in-codefence this}}{{#unless @last}} | {{/unless}}
  {{~/each}}
{{~else if (isEqual kind 184)}}{{! IntersectionType ~}}
  {{~#each types~}}
    {{>type-in-codefence this}}{{#unless @last}} & {{/unless}}
  {{~/each}}
{{~else if (isEqual kind 192)}}{{! LiteralType ~}}
  {{~literal}}
{{~else if (includes (list "String" "Boolean" "Number" "Null" "Unknown" "Any") this)}}
  {{~this}}
{{~else if (isEqual this (empty "object"))~}}
{{~else}}
  {{~log "unknown type" this}}
{{~/if}}
